{
  "name": "plcdnl/videos",
  "description": "Connect to YouTube & Vimeo and publish social videos on your website.",
  "version": "4.0.0",
  "type": "craft-plugin",
  "license": "proprietary",
  "keywords": [
    "vimeo",
    "youtube",
    "videos",
    "cms",
    "craftcms"
  ],
  "authors": [
    {
      "name": "plcdnl",
      "homepage": "https://plcd.nl/"
    }
  ],
  "support": {
    "docs": "https://github.com/plcdnl/videos/tree/v3/docs/docs",
    "source": "https://github.com/plcdnl/videos",
    "issues": "https://github.com/plcdnl/videos/issues?state=open"
  },
  "require": {
    "php": "^8.0.2",
    "craftcms/cms": "^5.0.0",
    "league/oauth2-client": "^2.5.0",
    "saf33r/oauth2-vimeo": "^0.3.0",
    "league/oauth2-google": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "plcdnl\\videos\\": "src/"
    }
  },
  "extra": {
    "name": "Videos",
    "handle": "videos"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "require-dev": {
    "craftcms/phpstan": "dev-main"
  },
  "scripts": {
    "phpstan": "phpstan --memory-limit=1G"
  }
}